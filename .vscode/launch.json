  "version": "0.2.0",
  "configurations": [
    {
      "name": "gcc - Build and debug project",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/scanner",
      "windows": {
        "program": "${workspaceFolder}\\scanner.exe"
      },
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: gcc build project",
      "miDebuggerPath": "gdb"
    }
  ]
}
